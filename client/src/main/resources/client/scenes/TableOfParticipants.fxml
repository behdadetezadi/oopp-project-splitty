<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Pagination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="335.0" prefWidth="594.0" stylesheets="@/styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.TableOfParticipantsController">
    <children>
        <Label fx:id="titleLabel" layoutX="219.0" layoutY="30.0" text="Participant Overview" textAlignment="CENTER" underline="true">
            <font>
                <Font size="29.0" />
            </font>
        </Label>
        <Pagination fx:id="pagination" layoutX="20.0" layoutY="62.0" pageCount="10" prefHeight="248.0" prefWidth="553.0" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="297.0" mnemonicParsing="false" onAction="#handleBackButton" text="Leave" />
      <Button fx:id="addButton" layoutX="479.0" layoutY="296.0" mnemonicParsing="false" onAction="#handleEditButton" prefHeight="26.0" prefWidth="94.0" text="Modify" />
      <Button fx:id="editButton" layoutX="539.0" layoutY="258.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="26.0" prefWidth="27.0" text="+" />
      <Button fx:id="deleteButton" layoutX="481.0" layoutY="258.0" mnemonicParsing="false" onAction="#handleDeleteButton" prefHeight="26.0" prefWidth="27.0" text="-" />
    </children>
</AnchorPane>
