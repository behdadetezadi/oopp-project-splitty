<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="335.0" prefWidth="594.0" stylesheets="@/styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.ContactDetailsCtrl">
   <children>
      <VBox alignment="CENTER" layoutX="25.0" layoutY="-21.0" prefHeight="453.0" prefWidth="543.0" spacing="10.0">
         <children>
            <Label prefHeight="9.0" prefWidth="468.0" styleClass="title" text="Contact Details">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label prefWidth="169.0" styleClass="subtitle" text="Add/Edit Participant" />
            <TextField fx:id="userNameField" promptText="User Name" />
            <TextField fx:id="firstNameField" promptText="First Name" />
            <TextField fx:id="lastNameField" promptText="Last Name" />
            <TextField fx:id="emailField" promptText="Email" />
            <TextField fx:id="ibanField" promptText="IBAN" />
            <TextField fx:id="bicField" promptText="BIC" />
            <HBox alignment="CENTER" spacing="10.0">
               <Button fx:id="cancelButton" onAction="#clearFields" styleClass="button" text="Cancel" />
               <Button fx:id="contactButton" onAction="#ok" styleClass="button" text="Ok" />
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
